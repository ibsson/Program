#include <stdio.h>

void BubbleSort(int data[], int len) {

	int tmp = 0;

	for (int i = 0; i < (len - 1); i++) {
		for (int j = 0; j < (len - (i + 1)); j++) {
			if (data[j] > data[j + 1]) {
				tmp = data[j + 1];
				data[j + 1] = data[j];
				data[j] = tmp;
			}
		}
	}
}

int main(void) {

	int data[] = { 6, 4, 2, 3, 1, 5 };
	int len = sizeof(data) / sizeof(data[0]);

	BubbleSort(data, len);

	for (int i = 0; i < len; i++) {
		printf("%d ", data[i]);
	}

	printf("\n");

	return 0;
}
