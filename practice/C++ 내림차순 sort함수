#include <iostream>
#include <algorithm>
using namespace std;

bool Compare(int a, int b){ //내림차순 정렬
	return a > b;
}

int main(void) {

	int n = 0;
  int num[100] = { 0 };

  cin >> n;

  for(int i = 0; i < n; i++){
    cin >> num[i];
  }

  sort(num, num + n, Compare);

  for(int i = 0; i < n; i++){
    cout << num[i] << " ";
  }
	
	return 0;
}
